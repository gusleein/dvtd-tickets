{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@core/*": [
        "app/modules/core/*"
      ],
      "@modules/*": [
        "app/modules/*"
      ],
      "@models": [
        "app/shared/models/index"
      ],
      "@types": [
        "app/shared/types"
      ],
      "@services/*": [
        "app/shared/services/*"
      ],
      "@stores/*": [
        "app/shared/stores/*"
      ],
      "@env/*": [
        "environments/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
